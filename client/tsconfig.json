{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "types": ["vite/client", "node"],
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["./src/**/*"]
}
