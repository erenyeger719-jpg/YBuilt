  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "echo \"(optional) if you compile TS, wire it here\"",
    "build:client": "vite build --config client/vite.config.js",
    "start": "tsx server/index.ts",
    "smoke": "node server/tests/smoke.mjs",
    "smoke:prod": "node -e \"if(!process.env.API) throw new Error('Set API=https://your-api-host');\" && BASE_URL=$API node server/tests/smoke.mjs",
    "smoke:sup": "node server/tests/sup-algo.smoke.mjs",
    "smoke:sup:prod": "node -e \"if(!process.env.API) throw new Error('Set API=https://your-api-host');\" && BASE_URL=$API node server/tests/sup-algo.smoke.mjs",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "typecheck:client": "tsc -p client/tsconfig.json --noEmit",
    "typecheck:server": "tsc -p server/tsconfig.json --noEmit",
    "test": "vitest run --config server/vitest.config.ts",
    "test:watch": "vitest -c server/vitest.config.mjs",
    "test:server": "vitest run --config server/vitest.config.ts",
    "typecheck:tests": "tsc -p server/tsconfig.test.json --noEmit",
    "sup:regress": "node tests/sup_algo.regression.mjs http://localhost:5050",
    "test:unit": "vitest run -c server/vitest.config.mjs",
    "test:cov": "vitest run -c server/vitest.config.mjs --coverage",
    "test:all": "npm run test:unit && npm run sup:regress"
  },
